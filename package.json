{
    "name": "todo",
    "packageManager": "yarn@3.3.0",
    "private": true,
    "scripts": {
        "build:backend": "yarn workspace @todo/backend npx prisma migrate reset --force && tsc",
        "build:frontend": "yarn workspace @todo/frontend tsc -b && vite build",
        "dev:backend": "yarn workspace @todo/backend nodemon src/index.ts",
        "dev:frontend": "yarn workspace @todo/frontend vite",
        "git:push": "git push origin $(git symbolic-ref --short HEAD)",
        "publish:frontend": "yarn workspace @todo/frontend publish --access public",
        "start:frontend": "yarn workspace @todo/frontend vite preview",
        "version:frontend": "yarn workspace @todo/frontend version patch --new-version $VERSION"
    },
    "workspaces": [
        "packages/*"
    ],
    "version": "0.0.1",
    "repository": "git@github.com:Chieze-Franklin/todo.git",
    "author": "Franklin Chieze <chieze.franklin@gmail.com>",
    "license": "MIT"
}